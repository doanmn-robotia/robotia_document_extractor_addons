<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- One-time Job: Update Title Row Selection Values -->
        <record id="ir_cron_update_title_selections" model="ir.cron">
            <field name="name">Update Title Row Selections (One-time)</field>
            <field name="model_id" ref="model_document_extraction"/>
            <field name="state">code</field>
            <field name="code">model.update_all_title_selections()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_admin"/>
        </record>
    </data>
</odoo>
